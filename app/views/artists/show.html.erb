
<h1><%= @artist.name %></h1>
<br />
<%= link_to 'Edit Artist', edit_artist_path(@artist) %>
<%= link_to 'Delete Artist', @artist, method: :delete %>
<%= link_to 'Back to Artists', artists_path(@artist) %>
<%= link_to 'Add a New Song', new_artist_song_path(@artist) %>
<% @songs.each do |song| %>
  <p><%= song.name %></p>
<% end %>